        <activity android:name=".activity.MyApplysActivity"></activity>
        <activity android:name=".activity.ApplyDetailActivity"></activity>
        <activity android:name=".activity.NewApplyActivity"></activity>
        <activity android:name=".activity.StatisticsActivity"></activity>
