        <activity android:name=".activity.ApproveDetailActivity"></activity>
        <activity android:name=".activity.CanteenListActivity"></activity>
        <activity android:name=".activity.TogetherPeopleActivity"></activity>

        <activity android:name=".activity.CaldroidSampleActivity"></activity>
