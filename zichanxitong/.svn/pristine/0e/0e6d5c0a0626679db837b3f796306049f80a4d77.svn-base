<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff"
    android:focusableInTouchMode="true"
    android:focusable="true"
    android:orientation="vertical">



    <com.szcomtop.meal.views.HeadView
        android:id="@+id/carteen_list_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />


    <com.szcomtop.meal.views.SearchBar
        android:id="@+id/carteen_list_search_bar"
        android:layout_width="match_parent"
        android:layout_height="35dp"
        android:layout_marginTop="7dp"
        android:layout_marginBottom="7dp"/>

    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="@color/search_bar_bg_color"/>


    <include layout="@layout/carteen_list_child" android:id="@+id/carteen_list_none"
        android:background="@drawable/option_item_selector"
        />


    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="@color/search_bar_bg_color"/>

    <ExpandableListView
        android:id="@+id/carteen_list_expanable_list"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:divider="@color/search_bar_bg_color"
        android:dividerHeight="1px"
        android:groupIndicator="@null">

    </ExpandableListView>



</LinearLayout>